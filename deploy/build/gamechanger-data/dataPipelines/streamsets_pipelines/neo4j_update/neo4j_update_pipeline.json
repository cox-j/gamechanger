      "configuration" : [ {
        "name" : "config.environmentVariables",
        "value" : [ {
          "key" : "SCRIPT_ENV",
          "value" : "prod"
        }, {
          "key" : "REPO_TGZ_NAME",
          "value" : "gamechanger-repo.dev.20210114.tgz"
        }, {
